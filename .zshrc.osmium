#!/usr/bin/env zsh

## home desktop

# re-export
export QCPROGS="${HOME}/data/opt/apps"
export apps="${QCPROGS}"

export LD_LIBRARY_PATH="${HOME}"/data/opt/lib:"${LD_LIBRARY_PATH}"

eval $(keychain --nogui --eval --quiet --agents ssh id_rsa)

export WORKON_HOME=$HOME/data/virtualenvs
source /usr/bin/virtualenvwrapper.sh

source /etc/profile.d/modules.zsh
module use "${HOME}/modules/osmium"
