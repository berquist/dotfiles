#!/usr/bin/env zsh

if [ -n "${__SOURCED_ZPROFILE}" ]; then return; fi
echo "running zprofile"
export __SOURCED_ZPROFILE=1

if [[ "$TERM" != "" ]]; then
    stty -ixon
fi

uname=$(uname)

if [[ $uname == "Darwin" ]]; then
    [[ -f /opt/homebrew/bin/brew ]] && eval "$(/opt/homebrew/bin/brew shellenv)"
fi
